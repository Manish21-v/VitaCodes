package com.manish;

public class AccessType {
	private void fun1()
	{
		System.out.println("inside fun1 method");
	}
	
	protected void fun2()
	{
		System.out.println("inside fun2 method");
	}
	
	void fun3()
	{
		System.out.println("inside fun3 method");
	}
	
	public void fun4()
	{
		System.out.println("inside fun4 method");
	}
	
}

 class Demo extends AccessType
{
	void display()
	{
		//fun1();
		fun2();
		fun3();
		fun4();
	}
	
} 